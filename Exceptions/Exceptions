class ClienteNFError(Exception):
    def __init__(self, message='Cliente não encontrado.'):
        self.message = message
        super().__init__(self.message)


class FilmeNFError(Exception):
    def __init__(self, message='Filme não encontrado.'):
        self.message = message
        super().__init__(self.message)


class LocNFError(Exception):
    def __init__(self, message='Locação não encontrada.'):
        self.message = message
        super().__init__(self.message)


class ResNFError(Exception):
    def __init__(self, message='Reserva não encontrada.'):
        self.message = message
        super().__init__(self.message)
